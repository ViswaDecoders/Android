<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.neeru.app8_combined1_7.Fifth"
    android:background="#d071e3">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wifi on"
        android:id="@+id/button10"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:background="#34f112" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="wifi off"
        android:id="@+id/button11"
        android:layout_alignBottom="@+id/button10"
        android:layout_alignParentEnd="true"
        android:background="#f64242" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton10"
        android:scaleType="fitCenter"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/prev"
        android:layout_alignEnd="@+id/button10"
        android:background="#d071e3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton11"
        android:scaleType="fitCenter"
        android:layout_alignBottom="@+id/imageButton10"
        android:layout_alignParentEnd="true"
        android:src="@drawable/next"
        android:layout_alignStart="@+id/button11"
        android:background="#d071e3" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toStartOf="@+id/button11"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/button10"
        android:layout_above="@+id/imageButton10">

        <ImageButton
            android:layout_width="191dp"
            android:layout_height="186dp"
            android:id="@+id/imageButton12"
            android:layout_gravity="center"
            android:src="@drawable/wifi_off" />
    </FrameLayout>

    <ToggleButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New ToggleButton"
        android:id="@+id/toggleButton2"
        android:textOff="wifi off"
        android:textOn="wifi on"
        android:checked="false"
        android:layout_alignTop="@+id/imageButton10"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp" />
</RelativeLayout>

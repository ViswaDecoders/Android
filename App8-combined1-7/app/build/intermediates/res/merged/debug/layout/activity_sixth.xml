<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.neeru.app8_combined1_7.Sixth"
    android:background="@color/abc_search_url_text_normal">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Torch on"
        android:id="@+id/button12"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:background="#34f112" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Torch off"
        android:id="@+id/button13"
        android:layout_alignBottom="@+id/button12"
        android:layout_alignParentEnd="true"
        android:background="#f64242" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton13"
        android:scaleType="fitCenter"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/prev"
        android:background="@color/abc_search_url_text_normal"
        android:layout_alignEnd="@+id/button12" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitCenter"
        android:id="@+id/imageButton14"
        android:layout_alignBottom="@+id/imageButton13"
        android:layout_alignEnd="@+id/button13"
        android:src="@drawable/next"
        android:background="@color/abc_search_url_text_normal"
        android:layout_alignStart="@+id/button13" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toStartOf="@+id/button13"
        android:layout_above="@+id/imageButton13"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/button12">

        <ImageButton
            android:layout_width="283dp"
            android:layout_height="194dp"
            android:id="@+id/imageButton15"
            android:scaleType="fitCenter"
            android:layout_gravity="center"
            android:src="@drawable/off2"
            android:adjustViewBounds="true"
            android:background="@color/abc_search_url_text_normal" />
    </FrameLayout>

    <ToggleButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New ToggleButton"
        android:textOn="torch on"
        android:textOff="torch on"
        android:checked="false"
        android:id="@+id/toggleButton3"
        android:layout_alignTop="@+id/imageButton13"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="44dp" />
</RelativeLayout>
